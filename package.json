{
  "name": "spotification",
  "version": "0.1.0",
  "description": "Spotify playlist desktop application",
  "main": "index.js",
  "scripts": {
    "start:ui": "cd src/ui && yarn start",
    "start:server": "ts-node src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd src/ui && yarn build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naasse/spotification.git"
  },
  "author": "Nathan Asselstine <Nate.Asselstine@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/naasse/spotification/issues"
  },
  "homepage": "https://github.com/naasse/spotification#readme",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "request": "^2.88.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^18.8.3",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "commitlint": "^17.1.2",
    "depcheck": "^1.4.3",
    "eslint": "^8.25.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*": [
      "yarn depcheck ."
    ],
    "src/ui/*": [
      "yarn depcheck src/ui"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "packageManager": "yarn@3.2.4"
}
